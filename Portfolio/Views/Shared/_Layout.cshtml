<body>
    <div id="wrap">


        <main class="cd-main-content">
            <div id="content">
                <div class="resume">
                    <div class="container">

                        <!-- TOP HEAD -->
                        <!-- Resume -->
                        <div class="row">
                            <!-- Sidebar -->
                            @Html.Action("AboutPerson", "Home")
                            <div class="col-md-8">
                                @Html.Partial("_Navbar")
                                @RenderBody()
                            </div>
                            <!-- MAIN CONTENT -->

                        </div>
                    </div>
                </div>
            </div>
        </main>


        <!-- Footer -->
        <footer class="footer">
            <div class="rights">
                <p>© Copyright 2016 MotiCV. All right reserved.</p>
            </div>
        </footer>
    </div>
    @RenderSection("script", required: false)
    <!-- End Footer -->
    <!-- End Page Wrapper -->
    <!-- JavaScripts -->
    <script src="~/Template/js/vendors/jquery/jquery.min.js"></script>
    <script src="~/Template/js/vendors/bootstrap.min.js"></script>
